{
    "appName": "site-api",
    "instanceCount": 1,
    "notExposeAsWebApp": false,
    "containerHttpPort": "$CODA_SITE_API_SERVER_PORT",
    "forceSsl": false,
    "envVars": [
        {
            "key": "CODA_SITE_API_SERVER_PORT",
            "value": "$CODA_SITE_API_SERVER_PORT"
        },
        {
            "key": "CODA_HUB_API_URL",
            "value": "$CODA_HUB_API_URL"
        },
        {
            "key": "CODA_STATS_API_URL",
            "value": "$CODA_STATS_API_URL"
        },
        {
            "key": "CODA_LEARNING_API_URL",
            "value": "$CODA_LEARNING_API_URL"
        },
        {
            "key": "CODA_AUTH_SERVICE_URL",
            "value": "$CODA_AUTH_SERVICE_URL"
        },
        {
            "key": "CODA_SITE_API_PROXY_URL",
            "value": "$CODA_SITE_API_PROXY_URL"
        },
        {
            "key": "CODA_SITE_API_HOSPITAL_CODE",
            "value": "$CODA_SITE_API_HOSPITAL_CODE"
        },
        {
            "key": "CODA_SITE_API_AUTH_REALM",
            "value": "$CODA_SITE_API_AUTH_REALM"
        },
        {
            "key": "CODA_SITE_API_AUTH_CLIENT_ID",
            "value": "$CODA_SITE_API_AUTH_CLIENT_ID"
        },
        {
            "key": "CODA_SITE_API_AUTH_USERNAME",
            "value": "$CODA_SITE_API_AUTH_USERNAME"
        },
        {
            "key": "CODA_SITE_API_AUTH_PASSWORD",
            "value": "$CODA_SITE_API_AUTH_PASSWORD"
        }
    ]
}